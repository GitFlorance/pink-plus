.map {
  margin-top: 45px;
  position: relative;
  display: flex;
  flex-direction: column;
  &__rec {
    position: absolute;
    z-index: 1;
    top: -45px;
    & svg {
      width: 100vw;
    }
  }
  iframe {
    width: 100vw;
  }
  &__outer {
    width: 100vw;
  }
  @media screen and (max-width: 768px) {
    iframe {
      height: 374px;
    }
    &__outer {
      height: 374px;
    }
  }
}