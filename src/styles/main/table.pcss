.table {
  background-color: #E5E5E5;
  margin: 140px 0px 80px;

  &__head {
    display: grid;
    width: 946px;
    grid-template-columns: 1fr 1fr 1fr 1fr;
    background-color: #E5E5E5;
    color: #FFFFFF;
    gap: 2px;

    &-none {
      background-color: #F2F2F2;
      border: 5px solid #F2F2F2;
    }
    &-blue {
      background-color: #283645;
      padding: 20px 0;
      //border: 2px solid #283645;
      position: relative;
      overflow: hidden;
    }
    &-pink {
      width: 150px;
      position: absolute;
      background-color: #D22856;
      color: #FFFFFF;
      font: 400 14px/30px 'Open Sans', sans-seri;
      text-align: center;
      transform-origin: center;
      transform: rotate(315deg);
      top: 15px;
      left: -45px;
    }
    &-name {
      font: 700 18px/30px 'Open Sans', sans-seri;
      text-align: center;
    }
    &-usd {
      font: 300 24px/30px 'Open Sans', sans-seri;
      text-align: center;
    }
  }
  &__white {
    width: 946px;
    display: grid;
    grid-template-columns: 1fr 1fr 1fr 1fr;
    background-color: #E5E5E5;
    gap: 2px;
    font: 400 18px/30px 'Open Sans', sans-seri;
    border: 2px solid #E5E5E5;
    border-radius: 1px;
    &-text {
      background-color: white;
      text-align: left;
      padding: 20px 20px;
    }
    &-svg {
      background-color: white;
      text-align: center;
      padding: 20px 0;
    }
  }
}
